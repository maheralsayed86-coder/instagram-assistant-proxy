# === Server ===
PORT=8080
X_API_KEY=change-this-secret

# === Redis (for scheduling via BullMQ) ===
REDIS_URL=redis://localhost:6379

# === Meta / Instagram Graph API ===
# Long-lived user/page token with required scopes (see README).
IG_ACCESS_TOKEN=YOUR_LONG_LIVED_TOKEN

# Default IG user id if you want to avoid passing in requests
DEFAULT_IG_USER_ID=1789xxxxxxxxxxxx

# API version - keep up to date with current Graph API version
GRAPH_API_VERSION=v23.0
